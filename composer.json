{
  "name": "dr/loginascustomer",
  "description": "Allows admin users to login as a specified customer.",
  "type": "magento2-module",
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    }
  ],
  "keywords": [
    "Magento 2",
    "Login as customer"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.magento.com/"
    }
  ],
  "require": {
    "php": "~5.5.0|~5.6.0",
    "magento/product-community-edition": "^1.0.0-beta2",
    "magento/magento-composer-installer": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.3.3",
    "phpmd/phpmd": "^2.2.3",
    "sebastian/phpcpd": "^2.0.2"
  },
  "extra": {
    "map": [
      [
        "src/*",
        "DR/LoginAsCustomer"
      ]
    ]
  }
}
